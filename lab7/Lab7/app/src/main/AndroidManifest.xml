        android:theme="@style/Theme.Lab6_MinMaxGame"
        tools:targetApi="31">
        <activity android:name="ru.leoonwi.lab7.MainMenuActivity" android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameActivity" android:exported="true">
        </activity>
    </application>

</manifest>